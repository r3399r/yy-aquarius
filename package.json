{
  "name": "sadalsuud",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "test": "ng test --code-coverage",
    "test:headless": "ng test --no-watch --no-progress --code-coverage --browsers=ChromeHeadless",
    "lint:ts": "ng lint",
    "fix:ts": "npm run lint:ts -- --fix",
    "lint:format": "prettier --config ./prettierrc.json --list-different \"./{src,e2e}/**/*.{js,ts,json,md,css,scss,html}\"",
    "fix:format": "npm run lint:format -- --write",
    "lint": "run-s lint:*",
    "fix": "run-s fix:*",
    "e2e": "ng e2e",
    "clean": "del-cli dist -f",
    "build:dev": "ng build",
    "build:prod": "ng build --prod",
    "pre:dev": "run-s clean fix build test:headless",
    "pre:commit": "run-s clean lint test:headless",
    "sync:dev": "aws s3 sync ./dist s3://sadalsuud-dev",
    "sync:prod": "aws s3 sync ./dist s3://sadalsuud-prod",
    "deploy:dev": "run-s clean build:dev sync:dev",
    "deploy:prod": "run-s clean build:prod sync:prod"
  },
  "dependencies": {
    "@angular/animations": "~11.0.0",
    "@angular/common": "~11.0.0",
    "@angular/compiler": "~11.0.0",
    "@angular/core": "~11.0.0",
    "@angular/forms": "~11.0.0",
    "@angular/platform-browser": "~11.0.0",
    "@angular/platform-browser-dynamic": "~11.0.0",
    "@angular/router": "~11.0.0",
    "@angular/service-worker": "~11.0.0",
    "@ionic/angular": "^5.5.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "uikit": "^3.5.9",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.1",
    "@angular/cli": "~11.0.1",
    "@angular/compiler-cli": "~11.0.0",
    "@ionic/angular-toolkit": "^3.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "del-cli": "^3.0.1",
    "husky": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "~4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre:commit"
    }
  }
}
